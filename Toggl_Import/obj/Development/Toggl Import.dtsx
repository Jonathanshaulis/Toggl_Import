      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{C6AA99F5-9151-408B-BBD6-2546DFC5CCC5}"
          SQLTask:SqlStatementSource="MERGE INTO archive.Toggl AS TogglTarget&#xA;USING (   SELECT ID&#xA;      , USER&#xA;      , Email&#xA;      , Client&#xA;      , Project&#xA;      , Task&#xA;      , [Description]&#xA;      , Billable&#xA;      , StartDate&#xA;      , StartTime&#xA;      , EndDate&#xA;      , EndTime&#xA;      , Duration&#xA;      , Tags&#xA;      , Amount&#xA;      , DurationDecimal&#xA;    FROM  dbo.Toggl) AS TogglSource (Toggl_ID, [User], Email, Client, Project, Task, [Description], Billable, StartDate, StartTime, EndDate, EndTime, Duration, Tags, Amount, DurationDecimal)&#xA;ON TogglSource.[User] = TogglTarget.[User]&#xA;   AND TogglSource.Email = TogglTarget.Email&#xA;   AND TogglSource.Client = TogglTarget.Client&#xA;   AND TogglSource.Project = TogglTarget.Project&#xA;   AND TogglSource.Task = TogglTarget.Task&#xA;   AND TogglSource.[Description] = TogglTarget.[Description]&#xA;   AND TogglSource.Billable = TogglTarget.Billable&#xA;   AND TogglSource.StartDate = TogglTarget.StartDate&#xA;   AND TogglSource.StartTime = TogglTarget.StartTime&#xA;   AND TogglSource.EndDate = TogglTarget.EndDate&#xA;   AND TogglSource.EndTime = TogglTarget.EndTime&#xA;   AND TogglSource.Duration = TogglTarget.Duration&#xA;   AND TogglSource.Tags = TogglTarget.Tags&#xA;   AND TogglSource.Amount = TogglTarget.Amount&#xA;   AND TogglSource.DurationDecimal = TogglTarget.DurationDecimal&#xA;WHEN MATCHED THEN UPDATE SET TogglTarget.Toggl_ID = TogglSource.Toggl_ID&#xA;WHEN NOT MATCHED THEN&#xA; INSERT (Toggl_ID&#xA;    , [User]&#xA;    , Email&#xA;    , Client&#xA;    , Project&#xA;    , Task&#xA;    , [Description]&#xA;    , Billable&#xA;    , StartDate&#xA;    , StartTime&#xA;    , EndDate&#xA;    , EndTime&#xA;    , Duration&#xA;    , Tags&#xA;    , Amount&#xA;    , DurationDecimal)&#xA; VALUES (TogglSource.Toggl_ID, TogglSource.[User], TogglSource.Email, TogglSource.Client, TogglSource.Project, TogglSource.Task, TogglSource.[Description]&#xA;    , TogglSource.Billable, TogglSource.StartDate, TogglSource.StartTime, TogglSource.EndDate, TogglSource.EndTime, TogglSource.Duration&#xA;    , TogglSource.Tags, TogglSource.Amount, TogglSource.DurationDecimal)&#xA;WHEN NOT MATCHED BY SOURCE AND DATEPART(YEAR, TogglTarget.StartDate) = (   SELECT TOP 1 DATEPART(YEAR, StartDate)&#xA;                     FROM  Toggl&#xA;                     ORDER BY StartDate DESC) THEN DELETE;&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
